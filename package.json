{
  "name": "cacheable-dns",
  "version": "0.3.3",
  "description": "",
  "keywords": [
    "dns"
  ],
  "bin": "lib/cli.js",
  "files": [
    "lib",
    "patches"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "repository": "git@github.com:BlackGlory/cacheable-dns.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "prepublishOnly": "run-s postinstall prepare clean build",
    "postinstall": "patch-package",
    "lint": "eslint --quiet src",
    "test": "vitest --run",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "smoke": "node lib/cli.js --help",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s postinstall prepare clean lint build smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.17.0",
    "@types/node": "18",
    "eslint": "^9.17.0",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "typescript-transform-paths": "^3.5.2",
    "vite": "^6.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "yarn-deduplicate": "^6.0.2"
  },
  "dependencies": {
    "@blackglory/errors": "^3.0.3",
    "@blackglory/prelude": "^0.4.0",
    "@extra-memoize/extra-disk-cache": "^0.8.3",
    "@extra-memoize/memory-cache": "^0.2.10",
    "brave-json": "^0.2.0",
    "buffercursor": "^0.0.12",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "extra-disk-cache": "^0.12.1",
    "extra-filesystem": "^0.5.1",
    "extra-logger": "^0.7.2",
    "extra-memoize": "^0.9.3",
    "extra-promise": "^7.0.0",
    "native-node-dns": "0.7.6",
    "native-node-dns-packet": "0.1.5",
    "patch-package": "^8.0.0",
    "return-style": "^3.0.1"
  }
}
